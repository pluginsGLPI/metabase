{
    "require": {
        "php": ">=8.2",
        "lcobucci/jwt": "^4.2"
    },
    "require-dev": {
        "glpi-project/tools": "^0.8"
    },
    "provide": {
        "guzzlehttp/guzzle": "*",
        "ext-sodium": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.99"
        },
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Glpi\\Tools\\": "../../tools/src/"
        }
    }
}
